{
    "defaultSeverity": "error",
    "jsRules": {},
    "rules": {
        "no-unused-variable": [
            true,
            {
                "ignore-pattern": "^_"
            }
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "ordered-imports": true
    },
    "rulesDirectory": []
}
